{
  "name": "@balancer-labs/frontend-v2",
  "version": "1.83.8",
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "vite": "vite",
    "dev": "npm run generate:tokenlists && vite",
    "serve": "npm run dev",
    "build": "npm run generate:tokenlists && vite build",
    "build:withouttokenlists": "vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "vitest": "vitest",
    "test:unit": "NODE_ENV=test vitest",
    "test:unit:watch": "NODE_ENV=test vitest -- --watch",
    "test:unit:coverage": "vitest --coverage",
    "lint": "eslint . --ext .js,.ts,.vue --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "import-abis": "cp ../core/deployments/artifacts/dockerParity/*.json ./src/utils/balancer/abi/.",
    "import-config": "tsc ./scripts/update-config.ts && node ./scripts/update-config.ts",
    "prettier": "prettier --check '**/*.*(md|json|yaml|html)' --loglevel warn",
    "prettier:fix": "npm run prettier -- --write",
    "stylelint": "stylelint '**/*.{vue,css}'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "tailwind:build": "tailwind-config-viewer export ./tailwind-static",
    "tailwind:view": "tailwind-config-viewer -o",
    "generate:tokenlists": "NODE_ENV=development npx vite-node --options.deps.inline=@ethersproject/basex --options.deps.inline=@ethersproject/signing-key ./src/lib/scripts/tokenlists.generator.ts",
    "generate:voting-gauges": "NODE_ENV=development npx vite-node --options.deps.inline=date-fns --options.deps.inline=@ethersproject/basex --options.deps.inline=@ethersproject/signing-key ./src/lib/scripts/voting-gauge.generator.ts",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/vue": "^6.4.2",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "axios": "^0.21.2",
    "bignumber.js": "^9.0.1",
    "msw": "^0.49.2",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.9.4",
    "vite-node": "^0.27.0",
    "vite-plugin-node-stdlib-browser": "^0.1.1",
    "vitest": "^0.27.0",
    "vue": "3.2.25",
    "vue-tsc": "^1.0.24"
  },
  "license": "MIT",
  "repository": "balancer-labs/frontend-v2"
}
